#gnome-settings-daemon &
export $(/usr/bin/gnome-keyring-daemon --start --components=pkcsll,secrets,ssh,gpg)

mon_primary='HDMI-0'
mon_secondary='DVI-D-0'
xrandr --output "$mon_primary" --primary --output "$mon_secondary" --right-of "$mon_primary"

xrdb -merge ~/.Xresources
bgimg='/mnt/win_data_hdd/Pictures/cats/Snoopy2.jpg'
multibg.py "$bgimg" "$bgimg"
setxkbmap -layout dvorak -option ctrl:nocaps -option compose:ralt
xset b off
numlockx

trayer --edge top --align right --SetDockType true --SetPartialStrut true --expand true \
       --width 5 --transparent true --alpha 0 --tint 0x002b36 --height 17 &
exec xmonad
